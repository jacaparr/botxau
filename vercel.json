{
    "version": 2,
    "name": "botxau-dashboard",
    "outputDirectory": "public",
    "builds": [
        {
            "src": "public/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}